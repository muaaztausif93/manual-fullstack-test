{
  "name": "server",
  "version": "1.0.0",
  "description": "backend for OpenHouse Task",
  "main": "server.ts",
  "engines": {
    "npm": ">=7.10.0",
    "node": "16.15.1"
  },
  "scripts": {
    "build": "tsc",
    "lint": "eslint src --ext .ts",
    "format": "eslint src --ext .ts --fix",

    "dev": "npm run migration:up:dev && NODE_ENV=development nodemon src/server.ts",
    "dev:watch": "nodemon src/server.ts",
    "start": "npm run build && npm run migration:up:dev && node build/src/server.js",
    "start:dev": "npm run migration:up:dev && NODE_ENV=development nodemon src/server.ts",
    "start:dev:ts-watch": "ts-node-dev --poll --respawn src/server.ts | pino-pretty -c -t",

    "migration:make": "npx knex migrate:make $npm_config_argv --migrations-directory ./src/migrations",
    "migration:up:dev": "NODE_ENV=development npx knex migrate:latest",
    "migration:down:dev": "NODE_ENV=development npx knex migrate:down",
    "migration:reset:dev": "NODE_ENV=development npx knex migrate:rollback --all && NODE_ENV=development knex migrate:latest",

    "start:docker:dev": "npm run docker-compose-up-dev",
    "stop:docker:dev": "npm run docker-compose-down-dev",
    "docker-compose-up-dev": "docker-compose -f ./docker-compose.yml --env-file .env.development up -d --build",
    "docker-compose-down-dev": "docker-compose -f ./docker-compose.yml --env-file .env.development down"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint"
    }
  },
  "author": "MuaazTausif",
  "license": "ISC",
